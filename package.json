{
  "name": "vue3-uniapp",
  "version": "0.0.1",
  "scripts": {
    "h5:dev": "npm-run-all --parallel mock h5:serve",
    "h5:serve": "uni --mode dev",
    "h5:deploy": "uni build --mode deploy",
    "h5:build": "uni build --mode prod",
    "wx:dev": "npm-run-all --parallel mock wx:serve",
    "wx:serve": "uni -p mp-weixin --mode dev",
    "wx:deploy": "uni build -p mp-weixin --mode deploy",
    "wx:build": "uni build -p mp-weixin --mode prod",
    "app:dev": "npm-run-all --parallel mock app:serve",
    "app:serve": "uni -p app --mode dev",
    "app:deploy": "uni build -p app --mode deploy",
    "app:build": "uni build -p app --mode prod",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src --fix",
    "mock": "cd mock && ts-node-dev mock.ts",
    "publish": "node ./bin/publish.js"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-3040820220424001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3040820220424001",
